\r\n      cart,\r\n      lineItems,\r\n      user,\r\n    } = this.state;\r\n    const buildLineItems = () => lineItems.map((item) => (\r\n      <SingleLineItem key={item.Id} item={item} />\r\n    ));\r\n    return (\r\n      <div>\r\n      <h1>Your Shopping Cart</h1>\r\n      <p>Here is your current order, {user.firstName}:</p>\r\n      {\r\n        (cart === null)\r\n          ? <div>\r\n          <p>Your cart is empty!</p>\r\n          <p>Please go to the Products page and click Add to Cart on an item to get started!</p>\r\n      </div>\r\n          : <div>\r\n          <h4>Total Price: ${cart.totalPrice}</h4>\r\n          <h4>Items:</h4>\r\n          <div>\r\n            <Table hover>\r\n              <thead>\r\n                <tr>\r\n                  <th>Product</th>\r\n                  <th>Price Per Unit</th>\r\n                  <th>Quantity</th>\r\n                  <th>Subtotal</th>\r\n                </tr>\r\n              </thead>\r\n              {buildLineItems()}\r\n            </Table>\r\n          </div>\r\n          </div>\r\n      }\r\n  </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default ShoppingCart;\r\n","C:\\Users\\Moniq\\source\\repos\\WackyPurseHunt\\WackyPurseHunt\\wackypursehunt.ui\\src\\helpers\\data\\customerData.js",["155"],"C:\\Users\\Moniq\\source\\repos\\WackyPurseHunt\\WackyPurseHunt\\wackypursehunt.ui\\src\\reportWebVitals.js",[],["156","157","158","159","160","161","162","163"],"C:\\Users\\Moniq\\source\\repos\\WackyPurseHunt\\WackyPurseHunt\\wackypursehunt.ui\\src\\helpers\\data\\connection.js",[],"C:\\Users\\Moniq\\source\\repos\\WackyPurseHunt\\WackyPurseHunt\\wackypursehunt.ui\\src\\components\\shared\\SingleLineItem\\SingleLineItem.js",[],"C:\\Users\\Moniq\\source\\repos\\WackyPurseHunt\\WackyPurseHunt\\wackypursehunt.ui\\src\\components\\pages\\NewArrivalCard\\NewArrivalCard.js",["164"],"import React from 'react';\r\n\r\nimport { Link } from 'react-router-dom';\r\n\r\nimport './NewArrivalCard.scss';\r\n\r\nclass NewArrivalC